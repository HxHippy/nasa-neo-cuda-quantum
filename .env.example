# NEO Quantum Configuration
# Copy this file to .env and fill in your values

# ============================================================================
# NASA API Configuration
# ============================================================================
# Get your free API key at: https://api.nasa.gov/
# DEMO_KEY works but is rate-limited to 30 requests/hour
NASA_API_KEY=DEMO_KEY

# NASA NEO API base URL (usually no need to change)
NASA_NEO_BASE_URL=https://api.nasa.gov/neo/rest/v1

# JPL APIs (public, no authentication required)
JPL_SENTRY_API_URL=https://ssd-api.jpl.nasa.gov/sentry.api
JPL_CAD_API_URL=https://ssd-api.jpl.nasa.gov/cad.api
JPL_SBDB_API_URL=https://ssd-api.jpl.nasa.gov/sbdb.api

# ============================================================================
# OpenRouter AI Configuration (Optional - for AI Fleet Reports)
# ============================================================================
# Get your API key at: https://openrouter.ai/
# Leave empty to disable AI analysis features
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_MODEL=nvidia/nemotron-3-nano-30b-a3b:free
OPENROUTER_HTTP_REFERER=https://neo-quantum.local

# ============================================================================
# Server Configuration
# ============================================================================
API_HOST=0.0.0.0
API_PORT=8000
WEBSOCKET_PATH=/ws/neo-stream

# Full WebSocket URL for frontend connection
# Change this if running behind a reverse proxy or in production
WEBSOCKET_URL=ws://localhost:8000/ws/neo-stream

# ============================================================================
# Quantum Circuit Configuration
# ============================================================================
# VQC (Variational Quantum Classifier)
QUANTUM_NUM_QUBITS=4
QUANTUM_NUM_LAYERS=3
QUANTUM_DEFAULT_SHOTS=1000
QUANTUM_BATCH_SHOTS=500
QUANTUM_HIGH_PRECISION_SHOTS=2000

# VQE (Variational Quantum Eigensolver)
VQE_NUM_QUBITS=6
VQE_DEFAULT_SHOTS=2000
VQE_MAX_ITERATIONS=50

# QAE (Quantum Amplitude Estimation)
QAE_NUM_QUBITS=8
QAE_PRECISION_QUBITS=4
QAE_DEFAULT_SHOTS=4000

# QAOA (Quantum Approximate Optimization)
QAOA_NUM_QUBITS=8
QAOA_DEFAULT_LAYERS=4
QAOA_DEFAULT_SHOTS=2000

# ============================================================================
# Training Configuration
# ============================================================================
TRAINING_DEFAULT_EPOCHS=30
TRAINING_DEFAULT_LEARNING_RATE=0.1
TRAINING_DEFAULT_SHOTS=500
TRAINING_TEST_SPLIT=0.2

# Data fetching limits (for training data)
SENTRY_FETCH_LIMIT=100
CAD_FETCH_LIMIT=500
PHA_FETCH_LIMIT=200

# ============================================================================
# Cache Configuration
# ============================================================================
# Directory for caching training data (relative to project root)
CACHE_DIR=src/training_cache
ENABLE_CACHE=true

# ============================================================================
# Debug/Development
# ============================================================================
DEBUG=false
LOG_LEVEL=INFO
